	author = {Kaur, Arshdeep and Verma, Deval and Kaur, Navneet},
	month = dec,
	year = {2022},
	keywords = {Software, Cloud computing, Data science, Gender gap, Industries, Instruments, Medical services, Pandemics, Salaries, Skills, Types, Visualization},
	pages = {1--4},
	file = {IEEE Xplore Abstract Record:/Users/11nho/Zotero/storage/2QIRXELR/10046491.html:text/html},
}

@article{quan_salary_2022,
	title = {Salary {Prediction} in {Data} {Science} {Field} {Using} {Specialized} {Skills} and {Job} {Benefits} – {A} {Literature} {Review}},
	volume = {6},
	abstract = {This paper aims to review the recent and existing methodologies for building a more suitable salary prediction model based on specialized skills and given job benefits in the Data Science field. The knowledge discovery also includes identifying existing human resource problems in the data science field and the most demanded skillset for early exploration and determination of input variables. As data science involves a high dimension of positions and responsibilities, the experimental dataset was projected to include skill-based and job benefits factors for more accurate salary predictions. The reviewed benchmarking machine learning methodologies on related problems are categorized into three main categories with individual strengths under different situations and requirements. Statistical methods are better in presenting variable relationships with extraordinary parameter tuning potential if linearity is present. Ensemble machine learning methods like Random Forest that combines multiple classifiers for more stable and accurate prediction. Deep learning-based neural networks have a strong specialty in handling unlabeled data and framework modifications. Moreover, it was realized that huge datasets with appropriate variables and grid search tuning method achieves greater and more reliable performance. However, extraordinary research on data science-related job benefits could be conducted if sufficient studies were present during that period. Overall, further work is necessary to determine the project’s objectives, scenario and experimental dataset to select suitable reviewed methodologies for the data science salary prediction model building.},
	language = {en},
	number = {3},
	author = {Quan, Tee Zhen and Raheem, Mafas},
	year = {2022},
	file = {Quan and Raheem - 2022 - Salary Prediction in Data Science Field Using Spec.pdf:/Users/11nho/Zotero/storage/EQZLJXM9/Quan and Raheem - 2022 - Salary Prediction in Data Science Field Using Spec.pdf:application/pdf},
}

@inproceedings{niknejad_analyzing_2023,
	title = {Analyzing {Data} {Professional} {Salaries} {Exploring} {Trends} and {Predictive} {Insights}},
	url = {https://ieeexplore.ieee.org/abstract/document/10339759},
	doi = {10.1109/BdKCSE59280.2023.10339759},
	abstract = {Many university students lack clear goals at the start of their academic journey, leading to a lack of motivation and active engagement in the classroom. This often results in difficulties understanding lectures, poor exam performance, and a failure to acquire essential skills. Despite this, finding a suitable job with a decent salary after graduation, particularly in data science, poses a significant challenge for university students. To enhance the motivation of data science students, we have obtained the Data Science Salaries 2023 dataset from the Kaggle website for exploring data science salaries through Exploratory Data Analysis (EDA). Furthermore, this study aims to uncover hidden patterns and extract valuable insights to predict the success factors for higher salaries by applying Linear Regression, KNN Regression, and Decision Tree Regression Models. The findings of this study could assist both university students and data scientists in comprehending the factors contributing to higher salaries, thereby enriching their knowledge for future career endeavours.},
	urldate = {2024-11-07},
	booktitle = {2023 {International} {Conference} on {Big} {Data}, {Knowledge} and {Control} {Systems} {Engineering} ({BdKCSE})},
	author = {Niknejad, Naghmeh and Kianiani, Marzieh and Puthiyapurayil, Nafeesath Parappurath and Khan, Talha Ali},
	month = nov,
	year = {2023},
	keywords = {Data science, Analytical models, Companies, Data Science Salaries, Data Visualization, Exploratory Data Analysis, Linear regression, Market research, Predictive models, Regression Model, Training},
	pages = {1--6},
}

@inproceedings{gill_predictive_2023,
	title = {Predictive {Human} {Resource} {Analytics} for {Classification} of {Salaries} in the {Field} of {Data} {Science}},
	url = {https://ieeexplore.ieee.org/abstract/document/10442025},
	doi = {10.1109/SMARTGENCON60755.2023.10442025},
	abstract = {The rapid growth of big data has led to a lack of jobs in the field of data science, resulting in a strong need for skilled experts to fill particular tasks in this domain. In order to get the aforementioned conclusions, an examination of the prevailing employment patterns in the field of data science was conducted, using a secondary dataset. The primary dashboard offered valuable analytics information for enhancing job security and fostering career advancement. Additionally, the study highlighted the key factors that are in high demand in the field of data science, which are crucial for constructing successful models. Various data pre-processing techniques were extensively used to effectively prepare and optimise the dataset for the stated goals of human resource analytics. The primary aim of this research is to do a thorough examination of human resource elements within the domain of data science employment. The present investigation will concentrate on the notable influence of specialised skill sets and their association with salary prediction. The results of this study demonstrate a considerable degree of precision, as seen by an 88 percent success rate achieved by the Random Forest Classifier.},
